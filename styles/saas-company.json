{
	"version": 2,
	"title": "Saas Company",
	"settings": {
        "typography": {
            "fluid": true,
            "fontSizes": [
                {
                    "fluid": false,
                    "slug": "tiny",
                    "size": "12px",
                    "name": "Tiny"
                },
                {
                    "fluid": false,
                    "slug": "extra-small",
                    "size": "13px",
                    "name": "Extra Small"
                },
                {
                    "fluid": false,
                    "slug": "small",
                    "size": "15px",
                    "name": "Small"
                },
                {
                    "slug": "normal",
                    "size": "clamp(17px, 1.063rem + ((1vw - 7.68px) * 0.361), 20px)",
                    "fluid": {
                        "min": "17px",
                        "max": "20px"
                    },
                    "name": "Normal"
                },
                {
                    "slug": "large",
                    "size": "clamp(40px, 2.5rem + ((1vw - 7.68px) * 1.442), 52px)",
                    "fluid": {
                        "min": "40px",
                        "max": "52px"
                    },
                    "name": "Large"
                },
                {
                    "slug": "medium-large",
                    "size": "clamp(40px, 2.5rem + ((1vw - 7.68px) * 2.404), 60px)",
                    "fluid": {
                        "min": "40px",
                        "max": "60px"
                    },
                    "name": "Medium Large"
                },
                {
                    "slug": "extra-large",
                    "size": "clamp(40px, 2.5rem + ((1vw - 7.68px) * 4.087), 74px)",
                    "fluid": {
                        "min": "40px",
                        "max": "74px"
                    },
                    "name": "Extra large"
                },
                {
                    "slug": "huge",
                    "size": "clamp(40px, 2.5rem + ((1vw - 7.68px) * 6.01), 90px)",
                    "fluid": {
                        "min": "40px",
                        "max": "90px"
                    },
                    "name": "Huge"
                }
            ]
        },
		"color": {
            "gradients": [
				{
					"slug": "primary-gradient",
					"gradient": "linear-gradient(to bottom, #503BDB 0%, #4535B3 100%)",
					"name": "Primary Gradient"
				}
			],
			"palette": [
                {
                    "slug": "background",
                    "color": "#FFFFFF",
                    "name": "Background"
                },
                {
                    "slug": "primary",
                    "color": "#503BDB",
                    "name": "Primary"
                },
                {
                    "slug": "secondary",
                    "color": "#151545",
                    "name": "Secondary"
                },
                {
                    "slug": "tertiary",
                    "color": "#4C4C75",
                    "name": "Tertiary"
                },
                {
                    "slug": "quaternary",
                    "color": "#F2EEFF",
                    "name": "Quaternary"
                },
                {
                    "slug": "quinary",
                    "color": "#F6B642",
                    "name": "Quinary"
                },
                {
                    "slug": "senary",
                    "color": "#503BDB",
                    "name": "Senary"
                }
            ],
            "duotone": [
                {
                    "colors": [
                        "#503BDB",
                        "#503BDB"
                    ],
                    "slug": "foreground-and-primary",
                    "name": "Foreground and primary"
                }
            ]
		},
        "custom": {
            "spacing": {
                "small": "1.25rem",
                "normal": "3.125rem",
                "medium": "clamp(2rem, 8vw,6.25rem)",
                "medium-large": "clamp(3.125rem, 12vw,9.375rem)",
                "large": "clamp(6.25rem, 16vw,12.5rem)",
                "single-post-feature-img-cover-minheight":"clamp(20rem, 20rem + ((1vw - 0.48rem) * 38.462), 40rem)",
                "single-post-cover-feature-img-border": "clamp(0.875rem, 0.875rem + ((1vw - 0.48rem) * 2.644), 2.25rem)",
                "btny":"15px",
                "btnx":"27px"
            },
            "typography": {
                "font-size": {
                    "hfive": "22px",
                    "hsix": "18px",
                    "p": "1rem",
                    "btn": "1rem",
                    "post-title": "clamp(32px, 2rem + ((1vw - 7.68px) * 0.721), 38px)",
                    "single-post-title": "clamp(32px, 2rem + ((1vw - 7.68px) * 0.721), 38px)",
                    "navigation":"15px"
                },
                "line-height": {
                    "post-title": "1.42",
                    "single-post-title": "1.42",
                    "navigation":"1.2",
                    "tiny":"1.8",
                    "extra-small":"1.8",
                    "small": "1.8",
                    "normal":"1.9",
                    "large":"1.35",
                    "medium-large":"1.4",
                    "extra-large":"1.3",
                    "huge":"1.25",
                    "button":"1.4"
                },
                "font-weight": {
                    "normal": 400,
                    "medium": 500,
                    "semi-bold": 600,
                    "bold": 700,
                    "heading":600,
                    "button":500,
                    "huge":600
                },
                "font-family": {
                    "primary": "var(--wp--preset--font-family--inter-font)",
                    "secondary": "var(--wp--preset--font-family--inter-font)"
                }
            },
            "color": {
                "navigation-text":"var(--wp--preset--color--secondary)",
                "navigation-link-hover":"var(--wp--preset--color--primary)"
            }
        }
	},
    "styles": {
        "elements": {
            "link": {
				"color": {
                    "text": "inherit"
                },
                "typography": {
					"textDecoration": "underline"
				},
				":hover": {
                    "color": {
                        "text": "var(--wp--preset--color--primary)"
                    },
					"typography": {
						"textDecoration": "none"
					}
				}
			},
            "h1": {
                "typography": {
                    "fontSize": "clamp(42px, 2.625rem + ((1vw - 7.68px) * 1.683), 56px)",
                    "lineHeight": "1.32"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "clamp(34px, 2.125rem + ((1vw - 7.68px) * 1.442), 46px)",
                    "lineHeight": "1.35"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "clamp(30px, 1.875rem + ((1vw - 7.68px) * 0.481), 34px)",
                    "lineHeight": "1.4"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "clamp(26px, 1.625rem + ((1vw - 7.68px) * 0.24), 28px)",
                    "lineHeight": "1.46"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--custom--typography--font-size--hfive)",
                    "lineHeight": "1.45"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--custom--typography--font-size--hsix)",
                    "lineHeight": "1.44"
                }
            }
        }
    }
}